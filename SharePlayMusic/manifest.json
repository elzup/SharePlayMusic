{
  "name":"SharePlayMusic",
  "version":"1",
  "manifest_version":2,
  "description":"You can share music",
  "permissions":[
    "tabs","contextMenus"
  ],
  "icons":{
    "16":"images/icon_16.png",
    "48":"images/icon_48.png",
    "128":"images/icon_128.png"
  },
  "content_scripts":[{
    "matches":["*://play.google.com/music/*"],
    "js":["js/jquery-2.2.0.min.js", "js/contentscript.js"]
  }],
  "background":{
    "scripts":["js/background.js"]
  }
}